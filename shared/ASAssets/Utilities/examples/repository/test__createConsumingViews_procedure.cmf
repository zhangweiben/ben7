PROCEDURE test_createConsumingViews(
&#x9;IN  recurse&#x9;&#x9;BIT,
&#x9;IN  overwrite&#x9;BIT,
&#x9;OUT success&#x9;&#x9;BIT
)
BEGIN
&#x9;DECLARE sourcepath &#x9;&#x9;VARCHAR(2048);
&#x9;DECLARE destpath &#x9;&#x9;VARCHAR(2048);

&#x9;set sourcepath = &apos;/shared/ASAssets/Utilities/examples/repository/source/ds_inventory/tutorial&apos;;
&#x9;set destpath = &apos;/shared/ASAssets/Utilities/examples/repository/target/tutorial&apos;;

&#x9;call print (&apos;CREATE CONSUMING VIEWS: Source=&apos;||sourcepath);
&#x9;call print (&apos;CREATE CONSUMING VIEWS: Destination=&apos;||destpath);
&#x9;call /shared/ASAssets/Utilities/repository/createConsumingViews(sourcepath, destpath, recurse, overwrite, success);
END</attribute>
