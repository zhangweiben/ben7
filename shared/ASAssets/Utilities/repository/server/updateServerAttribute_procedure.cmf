PROCEDURE updateServerAttribute(
&#x9;IN debug &#x9;&#x9;&#x9;&#x9;CHAR(1),-- e.g. Y or N
&#x9;IN ATTR &#x9;&#x9;&#x9;&#x9;VARCHAR(1024), -- e.g. /server/sql/language/caseSensitive
&#x9;IN ATTR_TYPE &#x9;&#x9;&#x9;VARCHAR(1024), -- e.g. STRING, MAP, LIST
&#x9;IN NEWVAL &#x9;&#x9;&#x9;&#x9;LONGVARCHAR
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;VARCHAR(255) DEFAULT &apos;updateServerAttribute&apos;;
&#x9;DECLARE request &#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE response &#x9;&#x9;XML;
&#x9;DECLARE fault &#x9;&#x9;&#x9;XML;
    DECLARE ex &#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;CASE 
&#x9;WHEN UPPER(ATTR_TYPE) = &apos;MAP&apos; THEN
&#x9;&#x9;set request =
    &#x9;&apos;&lt;ns1:updateServerAttributes &apos;||CHR(10)||
&#x9;&#x9;&apos; xmlns:ns1=&quot;http://www.compositesw.com/services/system/admin/server&quot; &apos;||CHR(10)||
&#x9;&#x9;&apos; xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot; &apos;||CHR(10)||
&#x9;&#x9;&apos; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;&apos;||CHR(10)||
    &#x9;&apos;    &lt;ns1:attributes&gt;&apos;||CHR(10)||
    &#x9;&apos;&#x9;    &lt;common:attribute xmlns:common=&quot;http://www.compositesw.com/services/system/util/common&quot;&gt;&apos;||CHR(10)||
    &#x9;&apos;            &lt;common:name&gt;&apos; || ATTR || &apos;&lt;/common:name&gt;&apos;||CHR(10)||
    &#x9;&apos;            &lt;common:type&gt;&apos; || ATTR_TYPE || &apos;&lt;/common:type&gt;&apos;||CHR(10)||
    &#x9;&apos;            &lt;common:valueMap&gt;&apos; || NEWVAL || 
/* This string format gets passed in as the NEWVAL variable
