&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;execute immediate sqlStatement;
&#x9;&#x9;&#x9;&#x9;set message = &apos;SUCCESS SETTING INACTIVE USERID=&apos;|| inUSERID;
&#x9;&#x9;&#x9;else
