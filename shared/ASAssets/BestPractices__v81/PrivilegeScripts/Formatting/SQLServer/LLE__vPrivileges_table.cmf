  <attribute name="SQL" type="STRING">SELECT PRIVILEGE_ID &#x9;&#x9;&#x9;&#x9;rownum, 
&#x9;0 AS &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;sheetRownum, 
&#x9;&apos;DB_LLE_SS&apos;&#x9;&#x9;&#x9;&#x9;&#x9;datasource,
&#x9;CAST(CASE 
&#x9;&#x9;WHEN TRIM(PRIVILEGE_TYPE) = &apos;SHARED&apos; &#x9;&#x9;THEN &apos;1Privileges_shared&apos;
&#x9;&#x9;WHEN TRIM(PRIVILEGE_TYPE) = &apos;DATABASES&apos; &#x9;THEN &apos;2Privileges_databases&apos;
&#x9;&#x9;WHEN TRIM(PRIVILEGE_TYPE) = &apos;WEBSERVICES&apos; &#x9;THEN &apos;3Privileges_webservices&apos;
&#x9;&#x9;ELSE TRIM(PRIVILEGE_TYPE)
&#x9;END as VARCHAR(255))&#x9;&#x9;sheetName, 
&#x9;CASE 
&#x9;&#x9;WHEN TRIM(PRIVILEGE_TYPE) = &apos;SHARED&apos; &#x9;&#x9;THEN &apos;1&apos;||PRIVILEGE_TYPE
&#x9;&#x9;WHEN TRIM(PRIVILEGE_TYPE) = &apos;DATABASES&apos; &#x9;THEN &apos;2&apos;||PRIVILEGE_TYPE
&#x9;&#x9;WHEN TRIM(PRIVILEGE_TYPE) = &apos;WEBSERVICES&apos; &#x9;THEN &apos;3&apos;||PRIVILEGE_TYPE
&#x9;&#x9;ELSE TRIM(PRIVILEGE_TYPE)
&#x9;END &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;PRIVILEGE_TYPE, 
&#x9;ORGANIZATION_NAME,
&#x9;PROJECT_NAME, 
&#x9;SUBPROJECT_NAME,
&#x9;RESOURCE_PATH, 
&#x9;RESOURCE_TYPE, 
&#x9;RECURSE_DEPENDENCIES, 
&#x9;RECURSE_DEPENDENTS, 
&#x9;RECURSE_CHILD, 
&#x9;REVOKE_ALL,
&#x9;NAME, 
&#x9;NAME_TYPE, 
&#x9;DOMAIN_NAME, 
&#x9;ENVIRONMENT_NAME,
&#x9;-- OrderPrecedence: Force privilege rows with ALL NO ACCESS TO OCCUR FIRST since these privileges are used to initialize before setting actual privileges
&#x9;CASE
&#x9;&#x9;WHEN (READ_ACCESS IS NULL OR LENGTH(TRIM(READ_ACCESS)) = 0)
&#x9;&#x9; AND (WRITE_ACCESS IS NULL OR LENGTH(TRIM(WRITE_ACCESS)) = 0)
&#x9;&#x9; AND (EXECUTE_ACCESS IS NULL OR LENGTH(TRIM(EXECUTE_ACCESS)) = 0)
&#x9;&#x9; AND (SELECT_ACCESS IS NULL OR LENGTH(TRIM(SELECT_ACCESS)) = 0)
&#x9;&#x9; AND (UPDATE_ACCESS IS NULL OR LENGTH(TRIM(UPDATE_ACCESS)) = 0)
&#x9;&#x9; AND (INSERT_ACCESS IS NULL OR LENGTH(TRIM(INSERT_ACCESS)) = 0)
&#x9;&#x9; AND (DELETE_ACCESS IS NULL OR LENGTH(TRIM(DELETE_ACCESS)) = 0)
&#x9;&#x9; AND (GRANT_ACCESS IS NULL OR LENGTH(TRIM(GRANT_ACCESS)) = 0)
&#x9;&#x9;THEN &apos;A&apos;
&#x9;&#x9;ELSE &apos;Z&apos;
&#x9;END AS&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ORDER_PRECEDENCE, 
&#x9;READ_ACCESS,
&#x9;WRITE_ACCESS,
&#x9;EXECUTE_ACCESS,
&#x9;SELECT_ACCESS,
&#x9;UPDATE_ACCESS,
&#x9;INSERT_ACCESS,
&#x9;DELETE_ACCESS,
&#x9;GRANT_ACCESS,
&#x9;OWNER_NAME, 
&#x9;OWNER_DOMAIN, 
&#x9;COMMENTS,
&#x9;INITIALIZE,
&#x9;ISACTIVE
FROM /shared/ASAssets/BestPractices_v81/PrivilegeScripts/Metadata/Privileges_DB_LLE_SS/CMPPRV/dbo/COMPOSITE_PRIVILEGES
WHERE ISACTIVE = &apos;Y&apos;

