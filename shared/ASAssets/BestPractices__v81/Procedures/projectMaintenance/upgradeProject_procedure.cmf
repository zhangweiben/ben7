PROCEDURE upgradeProject(
&#x9;IN debug&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y or N
&#x9;IN upgradeToVersion&#x9;&#x9;DOUBLE,&#x9;&#x9;&#x9;-- the version to upgrade the project to
&#x9;IN projectPath&#x9;&#x9;&#x9;VARCHAR(4000),&#x9;-- the full path to a project including the project name
&#x9;IN scriptsPath&#x9;&#x9;&#x9;VARCHAR(4000), &#x9;-- the full path to a project scripts.  e.g. /shared/PROJECT1/_scripts
&#x9;IN excludeNames&#x9;&#x9;&#x9;LONGVARCHAR,&#x9;-- comma separated list of data source path name segments to exclude from the name of the groupId such as /dbo
&#x9;IN generateTestFolder&#x9;BIT,&#x9;&#x9;&#x9;-- 1=generate test folder, 0 or null (default)=do not generate test folder
&#x9;OUT message&#x9;&#x9;&#x9;&#x9;LONGVARCHAR
)
BEGIN
&#x9;DECLARE moduleName&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR DEFAULT &apos;upgradeProject&apos;;
&#x9;DECLARE debug2&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CHAR(1) DEFAULT &apos;N&apos;;
&#x9;DECLARE projectPathSrc&#x9;&#x9;&#x9;&#x9;VARCHAR(4000);&#x9;
&#x9;DECLARE projectPathDst&#x9;&#x9;&#x9;&#x9;VARCHAR(4000) DEFAULT projectPath;
&#x9;DECLARE projectPathDstReserved&#x9;&#x9;VARCHAR(4096);
&#x9;DECLARE upgradeVersionVector&#x9;&#x9;VECTOR(ROW(bestPracticesVersionFrom DOUBLE, bestPracticesVersionTo DOUBLE, backupType INTEGER)); -- backupType: [0=backup individual folders in place, 1=backup entire folder]
&#x9;DECLARE upgradeVector &#x9;&#x9;&#x9;&#x9;/shared/ASAssets/BestPractices_v81/Procedures/TypeDefinitions.upgradeVector;
&#x9;DECLARE upgradeVectorRow&#x9;&#x9;&#x9;/shared/ASAssets/BestPractices_v81/Procedures/TypeDefinitions.upgradeVector DEFAULT VECTOR[(null)];
&#x9;DECLARE bestPracticesRootPath&#x9;&#x9;VARCHAR(4000) DEFAULT /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.bestPracticesRootPath;
&#x9;DECLARE utilitiesRootPath&#x9;&#x9;&#x9;VARCHAR(4000) DEFAULT /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.utilitiesRootPath;
&#x9;DECLARE bestPracticesVersionDefault DOUBLE DEFAULT /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.bestPracticesVersion;
&#x9;-- Define the full template path
&#x9;DECLARE templatePath&#x9;&#x9;&#x9;&#x9;VARCHAR(4000) DEFAULT bestPracticesRootPath||&apos;/DataAbstraction_GENERIC_Template&apos;;
&#x9;DECLARE templatePathScriptsPath&#x9;&#x9;VARCHAR(4000) DEFAULT bestPracticesRootPath||&apos;/DataAbstraction_GENERIC_Template/_scripts&apos;;
&#x9;-- Define the Utilities documentation path
&#x9;DECLARE docDriverPath &#x9;&#x9;&#x9;&#x9;VARCHAR(4000) DEFAULT utilitiesRootPath||&apos;/documentation/getDocumentationDriver&apos;;
&#x9;DECLARE docRootPath&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR(4000) DEFAULT  /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.docDefaultRootPath;
&#x9;-- Define general variables
&#x9;DECLARE upgradeToVersion_&#x9;&#x9;&#x9;DOUBLE DEFAULT upgradeToVersion;
&#x9;DECLARE bestPracticesVersionCurrent&#x9;DOUBLE;
&#x9;DECLARE bestPracticesMinorVersionCurrent DOUBLE;
&#x9;DECLARE bestPracticesVersionFrom&#x9;DOUBLE;
&#x9;DECLARE bestPracticesVersionTo&#x9;&#x9;DOUBLE;
&#x9;DECLARE bestPracticesVersionVectFrom DOUBLE;
&#x9;DECLARE bestPracticesVersionVectTo&#x9;DOUBLE;
&#x9;DECLARE backupType&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE resourceAction&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE performedUpgrade&#x9;&#x9;&#x9;BOOLEAN DEFAULT false;
&#x9;DECLARE i,j&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER;
&#x9;DECLARE resultMessage&#x9;&#x9;&#x9;&#x9;VARCHAR(4000);
&#x9;DECLARE success&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;BIT; -- true(1) or false(0)
&#x9;DECLARE faultResponse &#x9;&#x9;&#x9;&#x9;XML;
&#x9;DECLARE createResponse &#x9;&#x9;&#x9;&#x9;XML;
&#x9;DECLARE result&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR(255);
&#x9;DECLARE resourcePath&#x9;&#x9;&#x9;&#x9;VARCHAR(4000);
&#x9;DECLARE resourceType&#x9;&#x9;&#x9;&#x9;VARCHAR;
&#x9;DECLARE resourcePathList &#x9;&#x9;&#x9;LONGVARCHAR;
&#x9;DECLARE version&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;DOUBLE;
&#x9;DECLARE minVersion&#x9;&#x9;&#x9;&#x9;&#x9;DOUBLE DEFAULT /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.minVersion;
&#x9;DECLARE resourceExists &#x9;&#x9;&#x9;&#x9;BOOLEAN;
&#x9;DECLARE eol&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;VARCHAR DEFAULT /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.LF;
&#x9;DECLARE messageNum&#x9;&#x9;&#x9;&#x9;&#x9;INTEGER DEFAULT 0;
&#x9;DECLARE ex&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION;

&#x9;-- Apply reserved word list to path
&#x9;call /shared/ASAssets/Utilities/repository/RepoUtils/applyReservedListToPath(projectPathDst, null, projectPathDstReserved);
&#x9;set projectPathDst = REPLACE(projectPathDst,&apos;&quot;&apos;,&apos;&apos;);

&#x9;/***************************************************************
&#x9; * Make sure the Utilities are up to date
&#x9; ***************************************************************/
&#x9;call /shared/ASAssets/Utilities/getUtilitiesVersion(version);
&#x9;if (version &lt;  minVersion) then
&#x9;&#x9;raise ex value moduleName||&apos;:  Utilities &apos;||minVersion||&apos; or higher is required&apos;;
&#x9;end if;
&#x9;
&#x9;/***************************************************************
&#x9; * Check to make sure the project exists
&#x9; ***************************************************************/
&#x9;call /lib/resource/ResourceExists(projectPathDst, &apos;CONTAINER&apos;, null, resourceExists);
&#x9;if (NOT resourceExists) then
&#x9;&#x9;raise ex value  moduleName||&apos;:  The project does not exist. Resource path=&apos;||NVL(projectPathDst,&apos;null&apos;);
&#x9;end if;

&#x9;/***************************************************************
&#x9; * Check to make sure the scriptsPath exists
&#x9; ***************************************************************/
&#x9;call /lib/resource/ResourceExists(scriptsPath, &apos;CONTAINER&apos;, null, resourceExists);
&#x9;if (NOT resourceExists) then
&#x9;&#x9;raise ex value  moduleName||&apos;:  The scriptsPath does not exist. Resource path=&apos;||NVL(scriptsPath,&apos;null&apos;);
&#x9;end if;

&#x9;/***************************************************************
&#x9; * Check to make sure the minimum version of the Best Practices is 7.0
&#x9; ***************************************************************/
&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/determineBestPracticesVersion(debug2, projectPathDst, scriptsPath, bestPracticesVersionCurrent, bestPracticesMinorVersionCurrent);
&#x9;if (bestPracticesVersionCurrent is null) then
&#x9;&#x9;raise ex value  moduleName||&apos;:  The project does not meet the minimum requirements for being BestPractices v1.0 or higher. Version cannot be determined.&apos;;
&#x9;end if;


&#x9;/************************************************************************
&#x9; * If the upgradeToVersion is null, assume the upgrade is to the current version.
&#x9; ************************************************************************/
&#x9;if (upgradeToVersion_ is null) then
&#x9;&#x9;set upgradeToVersion_ = bestPracticesVersionDefault;
&#x9;end if;

&#x9;-- Set the starting point version to the current version
&#x9;-- This is the lower boundary for the vector list.
&#x9;set bestPracticesVersionFrom = bestPracticesVersionCurrent;
&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;call print(moduleName||&apos; :***********************************************************************************************&apos;);
&#x9;&#x9;call print(moduleName||&apos; : Upgrade projectPathDst=[&apos;||projectPathDst||&apos;]&apos;);
&#x9;&#x9;call print(moduleName||&apos; :   projectPathDstReserved=&apos;||NVL(projectPathDstReserved,&apos;null&apos;));
&#x9;&#x9;call print(moduleName||&apos; :   scriptsPath=[&apos;||scriptsPath||&apos;]&apos;);
&#x9;&#x9;call print(moduleName||&apos; :   Current Best Practices Version=&apos;||NVL(CAST(bestPracticesVersionCurrent AS VARCHAR),&apos;null&apos;)||&apos;  upgradeToVersion=&apos;||NVL(CAST(upgradeToVersion_ AS VARCHAR),&apos;null&apos;));
&#x9;&#x9;call print(moduleName||&apos; :   Most Current Available Best Practices Version=&apos;||NVL(CAST(bestPracticesVersionDefault AS VARCHAR),&apos;null&apos;));
&#x9;&#x9;call print(moduleName||&apos; :***********************************************************************************************&apos;);
&#x9;&#x9;call pause(500);
&#x9;end if;

&#x9;/************************************************************************
&#x9; * Validate the version upgrading &quot;to&quot;
&#x9; ************************************************************************/
&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/vector_upgradeVersionVector(upgradeVersionVector);
&#x9;set i = 1;
&#x9;upgradeVersionValidateLoop:
&#x9;WHILE (i &lt;= CARDINALITY(upgradeVersionVector)) DO
&#x9;&#x9;if (upgradeVersionVector[i] is null) then
&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  upgradeVersionVector[&apos;||i||&apos;] may not be null.  Please set values properly.&apos;;
&#x9;&#x9;end if;
&#x9;&#x9;if (upgradeVersionVector[i].bestPracticesVersionFrom is null or upgradeVersionVector[i].bestPracticesVersionTo is null) then
&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  upgradeVersionVector[&apos;||i||&apos;].bestPracticesVersionFrom or upgradeVersionVector[&apos;||i||&apos;].bestPracticesVersionTo may not be null.  Please set values properly.&apos;;
&#x9;&#x9;end if;
&#x9;&#x9;-- If the upgradeToVersion matches the vector bestPracticesVersionTo then a match is found and valid.
&#x9;&#x9;if (upgradeToVersion_ = upgradeVersionVector[i].bestPracticesVersionTo) then
&#x9;&#x9;&#x9;leave upgradeVersionValidateLoop;
&#x9;&#x9;end if;
&#x9;&#x9;set i = i + 1;
&#x9;END WHILE;
&#x9;-- The upgradeToVersion was not found if the value of &quot;i&quot; is greater than the size of upgradeVersionVector 
&#x9;if (i &gt; CARDINALITY(upgradeVersionVector)) then
&#x9;&#x9;raise ex value  moduleName||&apos;:  Upgrade to version=[&apos;||NVL(CAST(upgradeToVersion_ AS VARCHAR),&apos;null&apos;)||&apos;] is not permitted.  This version does not exist in the upgrade list.&apos;;
&#x9;end if;

&#x9;/************************************************************************
&#x9; * Perform the upgrade looping through all version of the upgrade as needed.
&#x9; ************************************************************************/
&#x9;set message = &apos;Project [&apos;||NVL(projectPathDst,&apos;null&apos;)||&apos;]:  &apos;||eol;
&#x9;set i = 1;
&#x9;upgradeVersionExecLoop:
&#x9;WHILE (i &lt;= CARDINALITY(upgradeVersionVector)) DO

&#x9;&#x9;-- Initialize variables.
&#x9;&#x9;set upgradeVector = null;
&#x9;&#x9;set bestPracticesVersionVectFrom = upgradeVersionVector[i].bestPracticesVersionFrom;
&#x9;&#x9;set bestPracticesVersionVectTo = upgradeVersionVector[i].bestPracticesVersionTo;
&#x9;&#x9;set backupType = upgradeVersionVector[i].backupType;

&#x9;&#x9;-- When the rolling value of &quot;bestPracticesVersionFrom&quot; equals the vector &quot;bestPracticesVersionVectFrom&quot;
&#x9;&#x9;if (bestPracticesVersionFrom = bestPracticesVersionVectFrom) then
&#x9;&#x9;&#x9;set messageNum = messageNum + 1;
&#x9;&#x9;&#x9;set message = message || &apos;Message #&apos;||messageNum||&apos;: Upgrading &quot;From&quot; &apos;||bestPracticesVersionVectFrom||&apos; &quot;To&quot;: &apos;||bestPracticesVersionVectTo||&apos;.&apos;||eol;
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :  Message #&apos;||messageNum||&apos;: Upgrading &quot;From&quot; &apos;||bestPracticesVersionVectFrom||&apos; &quot;To&quot;: &apos;||bestPracticesVersionVectTo||&apos;.&apos;);
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : &apos;);
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    bestPracticesVersionFrom(&apos;||bestPracticesVersionFrom||&apos;) = bestPracticesVersionVectFrom(&apos;||bestPracticesVersionVectFrom||&apos;)&apos;);
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;/************************************************************************
&#x9;&#x9;&#x9; * If the backupType=1 then leave the original project folder in place then
&#x9;&#x9;&#x9; *   create a copy of the project to upgrade to.
&#x9;&#x9;&#x9; *   This happens when the bestPracticesVersionCurrent is:
&#x9;&#x9;&#x9; *     1.0, 5.0, or 6.0
&#x9;&#x9;&#x9; ************************************************************************/
&#x9;&#x9;&#x9;if (backupType = 1) then
&#x9;&#x9;&#x9;&#x9;-- Swap the actual project path to be the source and generate a new destination folder
&#x9;&#x9;&#x9;&#x9;set projectPathSrc = projectPathDst;
&#x9;&#x9;&#x9;&#x9;set projectPathDst = projectPathDst||&apos;_v&apos;||REPLACE(CAST(bestPracticesVersionDefault AS VARCHAR),&apos;.&apos;,&apos;&apos;);
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    New upgraded project folder=&apos;||projectPathDst);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;-- Generate the project
&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/generateProject(projectPathDst, scriptsPath, 1, 1, message);&#x9;
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Project Source=&apos;||NVL(projectPathSrc,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Project Destination=&apos;||NVL(projectPathDst,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Scripts Destination=&apos;||NVL(scriptsPath,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :***********************************************************************************************&apos;);
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;-- Invoke the procedure &quot;upgradeProjectVersionVector&quot; to get all of the vector entries for the given &quot;From&quot; and &quot;To&quot; versions
&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/vector_masterUpgradeVector(projectPathSrc, projectPathDst, scriptsPath, bestPracticesVersionVectFrom, bestPracticesVersionVectTo, upgradeVector);

&#x9;&#x9;&#x9;-- This is much like a linked list whereby the current &quot;To&quot; version becomes the next &quot;From&quot; version and so on.
&#x9;&#x9;&#x9;-- The &quot;bestPracticesVersionFrom&quot; becomes a rolling value that tracks the versions through the &quot;upgradeVersionVector&quot; list.
&#x9;&#x9;&#x9;set bestPracticesVersionFrom = bestPracticesVersionVectTo;

&#x9;&#x9;elseif (bestPracticesVersionVectTo &lt; upgradeToVersion_) then
&#x9;&#x9;&#x9;-- Keep going as long as the vector version &quot;bestPracticesVersionVectTo&quot; is &lt;= the target &quot;upgradeToVersion&quot;
&#x9;&#x9;&#x9;--   Once the &quot;upgradeToVersion&quot; &gt; the vector version then we are done.
&#x9;&#x9;&#x9;--   This establishes the upper boundary.
&#x9;&#x9;&#x9;--   The vector is what establishes the entire list of possible upgrades.  &#x9;

&#x9;&#x9;&#x9;-- This is much like a linked list whereby the current &quot;To&quot; version becomes the next &quot;From&quot; version and so on.
&#x9;&#x9;&#x9;-- The &quot;bestPracticesVersionFrom&quot; becomes a rolling value that tracks the versions through the &quot;upgradeVersionVector&quot; list.
&#x9;&#x9;&#x9;set bestPracticesVersionFrom = bestPracticesVersionVectTo;
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    bestPracticesVersionVectTo(&apos;||bestPracticesVersionVectTo||&apos;) &lt;= upgradeToVersion(&apos;||upgradeToVersion_||&apos;)&apos;);
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;else
&#x9;&#x9;end if;

&#x9;&#x9;IF (upgradeVector is null) THEN
&#x9;&#x9;&#x9;/************************************************************************
&#x9;&#x9;&#x9; * Skip the resoure action
&#x9;&#x9;&#x9; ************************************************************************/
&#x9;&#x9;&#x9;set messageNum = messageNum + 1;
&#x9;&#x9;&#x9;set message = message || &apos;Message #&apos;||messageNum||&apos;: Skipping upgrade &quot;From&quot; &apos;||bestPracticesVersionVectFrom||&apos; &quot;To&quot;: &apos;||bestPracticesVersionVectTo||&apos;.&apos;||eol;
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :  Message #&apos;||messageNum||&apos;: Skipping upgrade &quot;From&quot; &apos;||bestPracticesVersionVectFrom||&apos; &quot;To&quot;: &apos;||bestPracticesVersionVectTo||&apos;.&apos;);
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : &apos;);
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :-----------------------------------------------------------------------------------------------&apos;);
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : Upgrade Version:&apos;);
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :   upgradeVersionVector[&apos;||i||&apos;]:  bestPracticesVersionVectFrom=&apos;||bestPracticesVersionVectFrom||&apos;  bestPracticesVersionVectTo=&apos;||bestPracticesVersionVectTo||&apos;  backupType=&apos;||backupType);
&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :-----------------------------------------------------------------------------------------------&apos;);
&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;leave upgradeVersionExecLoop;
&#x9;&#x9;END IF;
&#x9;&#x9;set i = i + 1;
&#x9;END WHILE;

&#x9;&#x9;IF (upgradeVector IS NOT NULL) THEN
&#x9;&#x9;&#x9;/************************************************************************
&#x9;&#x9;&#x9; * Execute the resoure action using the upgrade vector
&#x9;&#x9;&#x9; ************************************************************************/
&#x9;&#x9;&#x9;-- Loop through the vector and execute the upgrade actions
&#x9;&#x9;&#x9;set j = 1;
&#x9;&#x9;&#x9;WHILE (j &lt;= CARDINALITY(upgradeVector)) DO
&#x9;&#x9;&#x9;&#x9;set upgradeVectorRow[1] = upgradeVector[j];

&#x9;&#x9;&#x9;&#x9;set resourceAction = upgradeVectorRow[1].resourceAction;
&#x9;&#x9;&#x9;&#x9;if (resourceAction IS NOT NULL AND LENGTH(&apos;|&apos;||resourceAction||&apos;|&apos;) &gt; 2) then  --notEmpty
&#x9;&#x9;&#x9;&#x9;&#x9;set performedUpgrade = true;
&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : &apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;-- action to take on the resource: 
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;     copy - copy srcResource to dstResource.  srcResource and dstResource required.  updateStruct is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                if resource type is CONTAINER then copy all resources.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                if resource type is not CONTAINER then copy specific resource.
&#x9;&#x9;&#x9;&#x9;&#x9;--   copyNotExist - copy srcResource to dstResource.  srcResource and dstResource required.  updateStruct is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                if resource type is CONTAINER then copy all resources if they do not exist.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                if resource type is not CONTAINER then copy specific resource if it does not exist.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;copyLeave - copy srcResource to _Copy_#.  srcResource is required.  dstResource and updateStruct are null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                Creates a copy of a resource and leaves the original in place. 
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                The copy detects other copies an increments the number as needed in the format of _Copy_#.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;   copyRename - copy srcResource to _Copy_#.  srcResource is required.  dstResource and updateStruct are null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                Creates a copy of a resource and renames the original to it.  The original is no longer present.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                The copy detects other copies an increments the number as needed in the format of _Copy_#.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9; copyChildren - copy the children of srcResource to dstResource.  srcResource and dstResource required.  updateStruct is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;                Only copying from CONTAINER to CONTAINER is allowed.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;   update - update dstResource using updateStruct.  srcResource is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--  updateTrigger - update all trigger resources found starting at dstResource using updateStruct.  srcResource is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;&#x9;&#x9;&#x9; dstResource can be a single trigger or folder of triggers.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;   updateCrud - update CRUD resources specified by dstResource using updateStruct.  srcResource is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;   delete - delete dstResource.  srcResource and updateStruct are null.
&#x9;&#x9;&#x9;&#x9;&#x9;--&#x9;&#x9;     move - move srcResource to dstResource.  updateStruct is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--     moveCustom - move all custom scripts from srcResource folder to the specified dstResource folder. updateStruct is null.
&#x9;&#x9;&#x9;&#x9;&#x9;--                  a custom resource is determined by finding a resource in dstResource that is not in the DataAbstraction_GENERIC_Template.
&#x9;&#x9;&#x9;&#x9;  BEGIN INDEPENDENT TRANSACTION

&#x9;&#x9;&#x9;&#x9;&#x9;CASE UPPER(resourceAction)
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;COPY&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeCopyResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;COPYNOTEXIST&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeCopyNotExistResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;COPYLEAVE&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeCopyLeaveResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;COPYRENAME&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeCopyRenameResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;COPYCHILDREN&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeCopyChildrenResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;UPDATE&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   dstResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeUpdateResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;UPDATETRIGGER&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   dstResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeUpdateTriggerResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;UPDATECRUD&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   dstResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeUpdateCrudResource(debug,scriptsPath,projectPathDst,projectPathSrc, projectPathDst, upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;DELETE&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   dstResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeDeleteResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;MOVE&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeMoveResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;MOVECUSTOM&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeMoveCustomResource(debug,scriptsPath,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;REBIND&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].srcResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeRebindResource(debug,upgradeVectorRow,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;CONFIGFOLDERS&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeConfigureStartingFolders(debug,upgradeVectorRow,excludeNames,success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;RESTOREDEFAULTVALUES&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeRestoreDefaultValues(debug, upgradeVectorRow[1].dstResource[1].&quot;path&quot;, success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN &apos;RESTOREDOCCONSTANTS&apos; THEN
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    Execute resourceAction[&apos;||LPAD(j, 3, &apos; &apos;)||&apos;]=&apos;||resourceAction|| &apos;   srcResource=&apos;||NVL(upgradeVectorRow[1].dstResource[1].&quot;path&quot;,&apos;null&apos;));
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/upgradeProjectProcedures/upgradeRestoreDocumentationConstants(debug, upgradeVectorRow[1].dstResource[1].&quot;path&quot;, success);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;&#x9;&#x9;END CASE;
&#x9;&#x9;&#x9;&#x9;&#x9;if (success = 0) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  Resource Action=[&apos;||resourceAction||&apos;] was not successful.&apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;  EXCEPTION
&#x9;&#x9;&#x9;&#x9; &#x9;  ELSE raise ex value  moduleName||&apos;:  &apos;||cast(CURRENT_EXCEPTION.TRACE as longvarchar);
&#x9;&#x9;&#x9;&#x9;  END;

&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;set j = j + 1;
&#x9;&#x9;&#x9;END WHILE;

&#x9;&#x9;&#x9;/************************************************************************
&#x9;&#x9;&#x9; * If the bestPracticesVersionCurrent is between 1.0 and 5.0 then
&#x9;&#x9;&#x9; *   generate a new ConfigureStartingFolders() procedure based on the 
&#x9;&#x9;&#x9; *   newly upgraded project.
&#x9;&#x9;&#x9;&#x9;&#x9;IN debug&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=debug, N=do not debug
&#x9;&#x9;&#x9;&#x9;&#x9;IN projectPath&#x9;&#x9;&#x9;VARCHAR(4000), &#x9;-- the full path to a project including the project name
&#x9;&#x9;&#x9;&#x9;&#x9;OUT message&#x9;&#x9;&#x9;&#x9;LONGVARCHAR
&#x9;&#x9;&#x9; ************************************************************************/
&#x9;&#x9;&#x9;if (bestPracticesVersionCurrent &lt; 6.0) then
&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/generateConfigurationStartingFolders(debug, scriptsPath, excludeNames, message);
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;/************************************************************************
&#x9;&#x9;&#x9; * If the bestPracticesVersionCurrent is &gt;= 6.0 and &lt;= 6.6 then
&#x9;&#x9;&#x9; *   upgrade the existing ConfigureStartingFolders() procedure from the 
&#x9;&#x9;&#x9; *   old project to the newly upgraded project.
&#x9;&#x9;&#x9;&#x9;&#x9;IN debug&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=debug, N=do not debug
&#x9;&#x9;&#x9;&#x9;&#x9;IN sourceProjectPath&#x9;VARCHAR(4000), &#x9;-- the full path to a source project including the project name
&#x9;&#x9;&#x9;&#x9;&#x9;IN targetProjectPath&#x9;VARCHAR(4000), &#x9;-- the full path to a target project including the project name
&#x9;&#x9;&#x9;&#x9;&#x9;OUT message&#x9;&#x9;&#x9;&#x9;LONGVARCHAR
&#x9;&#x9;&#x9; ************************************************************************/
&#x9;&#x9;&#x9;if (bestPracticesVersionCurrent &gt;= 6.0 and bestPracticesVersionCurrent &lt;= 6.6) then
&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/generateConfigurationStartingFoldersUpgrade(debug, projectPathSrc, scriptsPath, message);
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;/************************************************************************
&#x9;&#x9;&#x9; * If the bestPracticesVersionCurrent is &gt;= 7.0 and &lt; 8.0 and backupType = 1 (create a backup copy of the entire project instead of in-place copies) then
&#x9;&#x9;&#x9; *   generate a new ConfigureStartingFolders() procedure based on the 
&#x9;&#x9;&#x9; *   newly upgraded project and the existing data sources.
&#x9;&#x9;&#x9; *   save a copy of the original ConfigureStartingFolders().
&#x9;&#x9;&#x9;&#x9;&#x9;IN debug&#x9;&#x9;&#x9;&#x9;CHAR(1),&#x9;&#x9;-- Y=debug, N=do not debug
&#x9;&#x9;&#x9;&#x9;&#x9;IN projectPath&#x9;&#x9;&#x9;VARCHAR(4000), &#x9;-- the full path to a project including the project name
&#x9;&#x9;&#x9;&#x9;&#x9;OUT message&#x9;&#x9;&#x9;&#x9;LONGVARCHAR
&#x9;&#x9;&#x9; ************************************************************************/
&#x9;&#x9;&#x9;if (bestPracticesVersionCurrent &gt;= 7.0 and bestPracticesVersionCurrent &lt; 8.0 and backupType = 1) then
&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/projectMaintenance/generateConfigurationStartingFolders(debug, scriptsPath, excludeNames, message);
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;/***************************************************************
&#x9;&#x9;&#x9; * Rebind procedures found in /_scripts directory
&#x9;&#x9;&#x9; ***************************************************************/
&#x9;&#x9;&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;&#x9;&#x9;-- Rebind the generation scripts &quot;/_scripts&quot; path
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : &apos;);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/BestPractices_v81/Procedures/rebindGenerationScripts(debug, scriptsPath, templatePathScriptsPath, scriptsPath, result, resultMessage);
&#x9;&#x9;&#x9;&#x9;if (resultMessage IS NOT NULL AND LENGTH(&apos;|&apos;||resultMessage||&apos;|&apos;) &gt; 2) then  --notEmpty
&#x9;&#x9;&#x9;&#x9;&#x9;raise ex value cast(resultMessage as longvarchar);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;EXCEPTION
&#x9;&#x9;&#x9;&#x9;ELSE raise ex value cast(CURRENT_EXCEPTION.TRACE as longvarchar);
&#x9;&#x9;&#x9;END;

&#x9;&#x9;&#x9;/***************************************************************
&#x9;&#x9;&#x9; * Verify rebind paths have been updated
&#x9;&#x9;&#x9; ***************************************************************/
&#x9;&#x9;&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;&#x9;&#x9;-- Verify rebind worked correctly
&#x9;&#x9;&#x9;&#x9;for r as select * from /shared/ASAssets/Utilities/repository/getResourceListRecursive(scriptsPath, &apos;CONTAINER&apos;) do
&#x9;&#x9;&#x9;&#x9;&#x9;if (r.resType != &apos;CONTAINER&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :      INSPECTED: &apos;||r.resPath);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;for u as select * from /shared/ASAssets/Utilities/repository/getUsedResourcesCursor(r.resPath, r.resType) do
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (POSITION(templatePath||&apos;/&apos; IN u.resourcePath||&apos;/&apos;) &gt; 0) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (resourcePathList is null) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set resourcePathList = &apos;&apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set resourcePathList = resourcePathList || &apos;, &apos;;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :        NOT REBOUND&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set resourcePathList = resourcePathList || r.resPath;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end for;
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;end for;
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : &apos;);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; : &apos;);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;if (resourcePathList is not null) then
&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    verify:    resourcePathList not rebound=[&apos;||NVL(resourcePathList,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  The following resources were not rebound: &apos;||resourcePathList;
&#x9;&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    verify:    all resources successfully rebound.&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;EXCEPTION
&#x9;&#x9;&#x9;&#x9;ELSE raise ex value  moduleName||&apos;:  &apos;||cast(CURRENT_EXCEPTION.TRACE as longvarchar);
&#x9;&#x9;&#x9;END;

&#x9;&#x9;&#x9;/***************************************************************
&#x9;&#x9;&#x9; * Generate the Test folder if the user requested it
&#x9;&#x9;&#x9; ***************************************************************/
&#x9;&#x9;&#x9;if (generateTestFolder is not null and generateTestFolder = 1) then
&#x9;&#x9;&#x9;&#x9;call /lib/resource/ResourceExists(projectPathDst||&apos;/Test&apos;, &apos;CONTAINER&apos;, null, resourceExists);
&#x9;&#x9;&#x9;&#x9;if (NOT resourceExists) then
&#x9;&#x9;&#x9;&#x9;&#x9;BEGIN INDEPENDENT TRANSACTION
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- Copy the Application directory to Test
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    generateTest:      Copy Application folder=[&apos;||NVL(projectPathDst||&apos;/Test/Application&apos;,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/Utilities/repository/copyResources(templatePath||&apos;/Application&apos;, projectPathDst||&apos;/Test/Application&apos;, success, faultResponse);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (faultResponse IS NOT NULL) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  &apos;||cast(faultResponse as longvarchar);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- Copy the Business directory to Test
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    generateTest:      Copy Business folder=[&apos;||NVL(projectPathDst||&apos;/Test/Business&apos;,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/Utilities/repository/copyResources(templatePath||&apos;/Business&apos;, projectPathDst||&apos;/Test/Business&apos;, success, faultResponse);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (faultResponse IS NOT NULL) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  &apos;||cast(faultResponse as longvarchar);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;-- Copy the Physical directory to Test
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    generateTest:      Copy Physical folder=[&apos;||NVL(projectPathDst||&apos;/Test/Physical&apos;,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call /shared/ASAssets/Utilities/repository/copyResources(templatePath||&apos;/Physical&apos;, projectPathDst||&apos;/Test/Physical&apos;, success, faultResponse);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (faultResponse IS NOT NULL) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;raise ex value  moduleName||&apos;:  &apos;||cast(faultResponse as longvarchar);
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;&#x9;EXCEPTION
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE raise ex value  moduleName||&apos;:  &apos;||cast(CURRENT_EXCEPTION.TRACE as longvarchar);
&#x9;&#x9;&#x9;&#x9;&#x9;END;
&#x9;&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    generateTest:    Resource already exists=[&apos;||NVL(projectPathDst||&apos;/Test&apos;,&apos;null&apos;)||&apos;]&apos;);
&#x9;&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;else
&#x9;&#x9;&#x9;&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;&#x9;&#x9;&#x9;call print(moduleName||&apos; :    generateTest:    User chose not to create a Test folder.&apos;);
&#x9;&#x9;&#x9;&#x9;end if;
&#x9;&#x9;&#x9;end if;

&#x9;&#x9;&#x9;set messageNum = messageNum + 1;
&#x9;&#x9;&#x9;set message = message || &apos;Message #&apos;||messageNum||&apos;: Successfully upgraded from &apos;||NVL(cast(bestPracticesVersionVectFrom as varchar),&apos;null&apos;)||&apos; to &apos;||NVL(cast(bestPracticesVersionVectTo as varchar),&apos;null&apos;)||&apos;.&apos;||eol;
&#x9;&#x9;ELSE
&#x9;&#x9;&#x9;set messageNum = messageNum + 1;
&#x9;&#x9;&#x9;set message = message || &apos;Message #&apos;||messageNum||&apos;: Upgrade was not performed from &apos;||NVL(cast(bestPracticesVersionCurrent as varchar),&apos;null&apos;)||&apos; to &apos;||NVL(cast(bestPracticesVersionVectTo as varchar),&apos;null&apos;)||&apos;.&apos;||eol;
&#x9;&#x9;END IF;

&#x9;if (performedUpgrade) then
&#x9;&#x9;set messageNum = messageNum + 1;
&#x9;&#x9;set message = message || &apos;Message #&apos;||messageNum||&apos;: PERFORM ACTION: --&gt; Click the refresh button in Studio. &lt;--&apos;;
&#x9;end if;
&#x9;if (UPPER(debug) = &apos;Y&apos;) then
&#x9;&#x9;call print(moduleName||&apos; : &apos;||message);
&#x9;end if;
END</attribute>
