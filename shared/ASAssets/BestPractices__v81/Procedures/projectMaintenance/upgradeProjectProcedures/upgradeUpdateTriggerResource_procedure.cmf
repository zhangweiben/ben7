&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;set updateTrigger = true;
&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;if (UPPER(debug2) = &apos;Y&apos;) THEN
