&#x9;If you would like to obtain assistance with this software, such assistance may be obtained through a separate paid consulting
&#x9;agreement with TIBCO.

*/
PROCEDURE isUnsupportedColumnType(
&#x9;IN columnType&#x9;&#x9;VARCHAR,
&#x9;OUT isUnsupported&#x9;BIT
)
BEGIN
&#x9;DECLARE unsupportedColumnTypeList LONGVARCHAR DEFAULT /shared/ASAssets/BestPractices_v81/&quot;_ProjectMaintenance&quot;/defaultValues.unsupportedColumnTypeList;

&#x9;set isUnsupported = 0;
&#x9;for r as select * from /shared/ASAssets/Utilities/string/extractDelimitedText(unsupportedColumnTypeList,&apos;,&apos;,0,0,0,1,0) do
&#x9;&#x9;if (TRIM(UPPER(columnType)) = UPPER(r.object)) then
&#x9;&#x9;&#x9;set isUnsupported = 1;
&#x9;&#x9;end if;
&#x9;end for;
END</attribute>
